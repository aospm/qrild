package dpm;

const QMI_DPM_SERVICE = 47;

const QMI_DPM_OPEN_PORT = 0x20;

struct qmi_result {
	u16 result;
	u16 error;
};

struct control_port {
	u32 ep_type;
	u32 iface_id;
	u32 consumer_pipe_num;
	u32 producer_pipe_num;
};

request open_port_req {
	optional control_port port_list[1] = 0x11;
} = 0x20;

response open_port_resp {
	required qmi_result r = 2;
} = 0x20;
